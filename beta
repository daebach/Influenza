                    <Script isActive="yes" isFolder="no">
                        <name>piloting</name>
                        <packageName></packageName>
                        <script>apDir = apDir or &quot;north&quot;
apShape = apShape or &quot;square&quot;
apSize = apSize or 0
apMoves = apMoves or &quot;none&quot;
astarget = astarget or &quot;none&quot;


-- make it so we can reload script on fly without messing current values
apDir = apDir or &quot;north&quot;
apShape = apShape or &quot;square&quot;
apSize = apSize or 0
apMoves = apMoves or &quot;none&quot;

function apAutopilot()
if apMoves == apSize then
if apShape == &quot;square&quot; then
if apDir == &quot;north&quot; then
apDir = &quot;east&quot;
elseif apDir == &quot;east&quot; then
apDir = &quot;south&quot;
elseif apDir == &quot;south&quot; then
apDir = &quot;west&quot;
elseif apDir == &quot;west&quot; then
apDir = &quot;north&quot;
end
apMoves = 1
elseif apShape == &quot;diamond&quot; then
if apDir == &quot;northeast&quot; then
apDir = &quot;southeast&quot;
elseif apDir == &quot;southeast&quot; then
apDir = &quot;southwest&quot;
elseif apDir == &quot;southwest&quot; then
apDir = &quot;northwest&quot;
elseif apDir == &quot;northwest&quot; then
apDir = &quot;northeast&quot;
end
apMoves = 1
end
elseif apMoves &lt; apSize then
apMoves = apMoves + 1
end
tempTimer(0.2, [[send(&quot;pilot steer &quot;..apDir)]])

end</script>
