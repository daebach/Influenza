<packageName/>
<script>
empathPro = empathPro or { autoclear = autoclear or 1, chair = chair or 0, commandclear = commandclear or 0, cube = cube or 0, grid = grid or 0, gridclear = gridclear or 0, gridsetcheck = gridsetcheck or 0, orb = orb or 0, slivvener = slivvener or "none", turret1 = turret1 or 0, turret2 = turret2 or 0, turret3 = turret3 or 0, wingslivvener = wingslivvener or "none", slivvencount = 0, slivvencountcall = false, autoslivven = false, } function empDisplay(msg) cecho("\n<medium_orchid>[<lemon_chiffon>Daze<medium_orchid>]:") echo(" "..msg) end function empHeal() if empathPro.autoclear == 1 then send("grid clarity "..empathPro.chair, false) send("grid regenerate "..empathPro.chair, false) send("grid repair hull", false) send("grid repair module "..empathPro.grid, false) send("grid repair module "..empathPro.chair, false) send("grid repair module "..empathPro.turret1, false) send("grid repair module "..empathPro.turret2, false) send("grid repair module "..empathPro.turret3, false) send("grid repair module", false) empDisplay("HEALING") elseif empathPro.autoclear == 0 and empathPro.commandclear == 0 then send("grid regenerate "..empathPro.chair, false) send("grid repair hull", false) send("grid repair module "..empathPro.chair, false) send("grid repair module "..empathPro.grid, false) send("grid repair module "..empathPro.turret1, false) send("grid repair module "..empathPro.turret2, false) send("grid repair module "..empathPro.turret3, false) send("grid repair module", false) empDisplay("HEALING") elseif empathPro.autoclear == 0 and empathPro.commandclear == 1 then send("grid clarity "..empathPro.chair, false) send("grid regenerate "..empathPro.chair, false) send("grid repair hull", false) send("grid repair module "..empathPro.chair, false) send("grid repair module "..empathPro.grid, false) send("grid repair module "..empathPro.turret1, false) send("grid repair module "..empathPro.turret2, false) send("grid repair module "..empathPro.turret3, false) send("grid repair module", false) empDisplay("HEALING") empathPro.commandclear = 0 end end function empSlivvencall(module, person, id) local moduletype = module if module == "command" then moduletype = "command chair" elseif module == "grid" then moduletype = "empathic grid" elseif module == "orb" then moduletype = "shield orb" elseif module == "cube" then moduletype = "cloaking cube" elseif module == "turret" then moduletype = "battle turret" elseif module == "collector" then moduletype = "energy collector" end if autoslivven then send("tell "..empathPro.slivvener.." Slivven at "..id) elseif person then local peep = string.upper(string.sub(person, 0, 1)) .. string.lower(string.sub(person, 2)) if empathPro.slivvener ~= "none" then send("tell "..empathPro.slivvener.." Slivven at " .. peep .. "'s " .. moduletype, false) else send("shipt Slivven at " .. peep .. "'s " .. moduletype, false) end if empathPro.wingslivvener ~= "none" then send("tell "..empathPro.wingslivvener.." Slivven at " .. peep .. "'s " .. moduletype, false) end else if empathPro.slivvener ~= "none" then send("tell "..empathPro.slivvener.." Slivven at an unmanned " .. moduletype, false)	else send("shipt Slivven at an unmanned " .. moduletype, false)	end if empathPro.wingslivvener ~= "none" then send("tell " .. empathPro.wingslivvener .. " Slivven at an unmanned " .. moduletype, false) end end empathPro.slivvencount = empathPro.slivvencount + 1 end function slivvencall() if empathPro.slivvencount > 0 then send("shipt " .. empathPro.slivvencount .. " slivven found. Locations advised", false) elseif empathPro.slivvencount == 0 then send("shipt no slivven detected", false) end empathPro.slivvencountcall = false end function empGridSet() empathPro.chair = 0 empathPro.turret1 = 0 empathPro.turret2 = 0 empathPro.turret3 = 0 empathPro.grid = 0 empathPro.orb = 0 empathPro.cube = 0 send("GRID MODULES", false) empDisplay("setting up modules") empathPro.gridsetcheck = 1 end
