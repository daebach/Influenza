function empDisplay(msg)
	cecho(&quot;\n&lt;medium_orchid&gt;[&lt;lemon_chiffon&gt;Daze&lt;medium_orchid&gt;]:&quot;)
	echo(&quot; &quot;..msg)
end

function empHeal()

if empathPro.autoclear == 1 then
	send(&quot;grid clarity &quot;..empathPro.chair, false)
	send(&quot;grid regenerate &quot;..empathPro.chair, false)
	send(&quot;grid repair hull&quot;, false)
	send(&quot;grid repair module &quot;..empathPro.grid, false)
	send(&quot;grid repair module &quot;..empathPro.chair, false)
	send(&quot;grid repair module &quot;..empathPro.turret1, false)
	send(&quot;grid repair module &quot;..empathPro.turret2, false)
	send(&quot;grid repair module &quot;..empathPro.turret3, false)
	send(&quot;grid repair module&quot;, false)
	empDisplay(&quot;HEALING&quot;)
elseif empathPro.autoclear == 0 and empathPro.commandclear == 0 then
	send(&quot;grid regenerate &quot;..empathPro.chair, false)
	send(&quot;grid repair hull&quot;, false)
	send(&quot;grid repair module &quot;..empathPro.chair, false)
	send(&quot;grid repair module &quot;..empathPro.grid, false)
	send(&quot;grid repair module &quot;..empathPro.turret1, false)
	send(&quot;grid repair module &quot;..empathPro.turret2, false)
	send(&quot;grid repair module &quot;..empathPro.turret3, false)
	send(&quot;grid repair module&quot;, false)
	empDisplay(&quot;HEALING&quot;)
elseif empathPro.autoclear == 0 and empathPro.commandclear == 1 then
	send(&quot;grid clarity &quot;..empathPro.chair, false)
	send(&quot;grid regenerate &quot;..empathPro.chair, false)
	send(&quot;grid repair hull&quot;, false)
	send(&quot;grid repair module &quot;..empathPro.chair, false)
	send(&quot;grid repair module &quot;..empathPro.grid, false)
	send(&quot;grid repair module &quot;..empathPro.turret1, false)
	send(&quot;grid repair module &quot;..empathPro.turret2, false)
	send(&quot;grid repair module &quot;..empathPro.turret3, false)
	send(&quot;grid repair module&quot;, false)
	empDisplay(&quot;HEALING&quot;)
	empathPro.commandclear = 0
end

end

function empSlivvencall(module, person, id)
	local moduletype = module
	if module == &quot;command&quot; then
		moduletype = &quot;command chair&quot;
	elseif module == &quot;grid&quot; then
		moduletype = &quot;empathic grid&quot;
	elseif module == &quot;orb&quot; then
		moduletype = &quot;shield orb&quot;
	elseif module == &quot;cube&quot; then
		moduletype = &quot;cloaking cube&quot;
	elseif module == &quot;turret&quot; then
		moduletype = &quot;battle turret&quot;
	elseif module == &quot;collector&quot; then
		moduletype = &quot;energy collector&quot;
	end

	if autoslivven then
		send(&quot;tell &quot;..empathPro.slivvener..&quot; Slivven at &quot;..id)
	elseif person then
		local peep = string.upper(string.sub(person, 0, 1)) .. string.lower(string.sub(person, 2))
		if empathPro.slivvener ~= &quot;none&quot; then
			send(&quot;tell &quot;..empathPro.slivvener..&quot; Slivven at &quot; .. peep .. &quot;'s &quot; .. moduletype, false)
		else
			send(&quot;shipt Slivven at &quot; .. peep .. &quot;'s &quot; .. moduletype, false)
		end
		if empathPro.wingslivvener ~= &quot;none&quot; then
			send(&quot;tell &quot;..empathPro.wingslivvener..&quot; Slivven at &quot; .. peep .. &quot;'s &quot; .. moduletype, false)
		end
	else
		if empathPro.slivvener ~= &quot;none&quot; then
			send(&quot;tell &quot;..empathPro.slivvener..&quot; Slivven at an unmanned &quot; .. moduletype, false)		
		else
			send(&quot;shipt Slivven at an unmanned &quot; .. moduletype, false)	
		end
		if empathPro.wingslivvener ~= &quot;none&quot; then
			send(&quot;tell &quot; .. empathPro.wingslivvener .. &quot; Slivven at an unmanned &quot; .. moduletype, false)
		end
	end
	empathPro.slivvencount = empathPro.slivvencount + 1
end

function slivvencall()
	if empathPro.slivvencount &gt; 0 then
		send(&quot;shipt &quot; .. empathPro.slivvencount .. &quot; slivven found. Locations advised&quot;, false)
	elseif empathPro.slivvencount == 0 then
		send(&quot;shipt no slivven detected&quot;, false)
	end
	empathPro.slivvencountcall = false
end

function empGridSet()
empathPro.chair = 0
empathPro.turret1 = 0
empathPro.turret2 = 0
empathPro.turret3 = 0 
empathPro.grid = 0
empathPro.orb = 0 
empathPro.cube = 0
send(&quot;GRID MODULES&quot;, false) 
empDisplay(&quot;setting up modules&quot;) 
empathPro.gridsetcheck = 1
end
