                        <script>abalance = abalance or true
gargcount = gargcount or 0
karibcount = karibcount or 0
gargcounting = gargcounting or false

siphoning = siphoning or false

function aethergargcount(type)
	if type == &quot;death&quot; then
		send(&quot;shipt Beast slain. &quot;..gargcount..&quot; left&quot;, false)
	elseif type == &quot;summon&quot; then
		send(&quot;shipt Beast summoned. &quot;..gargcount..&quot; left&quot;)
	end
end

function siphonCheck()
	if gargcount &gt;= 2 or karibcount &gt; 0 then
		send(&quot;config shipsight 0&quot;, false)
	elseif gargcount &lt; 2 then
		send(&quot;config shipsight 1&quot;, false)
	end
end
