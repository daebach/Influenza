                        <script>local callList = {}
for i=1,#enterList do
	if ndb.ismag(enterList[i]) or ndb.isgaudi(enterList[i]) then
		callList[#callList + 1] = enterList[i]
	end
end

local callstring = &quot;&quot;
for i=1,#callList do
	if i == #callList - 1 then
		callstring = callstring .. callList[i] .. &quot; and &quot;
	elseif i == #callList then
		callstring = callstring .. callList[i]
	else
		callstring = callstring .. callList[i] .. &quot;, &quot;
	end
end

if #callList &gt; 0 then
	tcall(callstring .. &quot; enters &quot; .. gmcp.Room.Info.area)
end

enterList = {}

disableTrigger(&quot;Enemy enters end&quot;)</script>
