<script>
--Influenza System Settings --Setting up namespace if not Influenza then Influenza = {} end --Setting up default values function Influenza:default() Influenza = {} Influenza.healing = {} --Influenza.healing.setting = false --Influenza.healing.sip = "75" --Influenza.healing.scroll = "70" --Influenza.healing.sparkle = "65" Influenza.target = {} Influenza.target.influence = "None" Influenza.target.debate = "None" Influenza.mindset = {"analytical", "pedantic", "cautious"} Influenza.debate = {"pettifoggery", "passion", "pontification"} Influenza.attitude = {"zealotry", "saintly", "lawyerly"} Influenza.type = {}	Influenza.type.influence = "none" Influenza.influence = { charity = {"begging", "supplication", "wheedling"}, empower = {"compliments", "admiration", "praise"}, weaken = {"teasing", "mockery", "derision"}, seduction = {"flattery", "charm", "beguiling"}, paranoia = {"rumors", "distrust", "conspiracies"}, amnesty = {"amnesty", "amnesty", "amnesty"}, village = { Serenwilde = {"lectures", "recitation", "oration"}, Celest = {"proselytizing", "preaching", "evangelizing"}, Hallifax = {"accordance", "collaboration", "assimilation"}, Gaudiguch = {"revolution", "liberty", "freedon"}, Glomdoring = {"manipulation", "intrigue", "trickery"}, Magnagora = {"shock", "awe", "brainwash"}, } } Influenza.imbue = {} Influenza.imbue.settings = false Influenza.imbue.figurine = "" Influenza.imbue.esteem = "" Influenza.imbue.settings2 = false Influenza.echo = function (self, args) cecho(string.format("%s", "\n<sienna>[<burlywood>Influenza<sienna>]<burlywood>: <SeaGreen>Info: <slate_grey>") .. args:title() .. "\n") end --Beast Healing values Influenza.beast = {} Influenza.beast.number = "nil" Influenza.beast.setting = false Influenza.configged = true --Herb Stores Influenza.rift = {} Influenza.herbstore = {} end --Influenza Guts --Influencing function Influenza:influencer() if not Influenza.influence then Influenza.influence = {} end if not Influenza.type.influence then cecho("<blue>\nNo influence type selected") else if not lastinfluence then lastinfluence = 1 elseif lastinfluence == 3 then lastinfluence = 1 else lastinfluence = lastinfluence + 1 end send("influence " .. Influenza.target.influence .. " with " .. Influenza.influence[Influenza.type.influence][lastinfluence]) end end --Debating function Influenza:debate() if not Influenza.debate then Influenza.debate = {} end if not Influenza.target.debate then cecho("<red>\n\nNo Target Defined - DEBT <PERSON>!!") end send("debate " ..Influenza.target.debate.. " with " .. Influenza.debate[math.random(#Influenza.debate)]) end --Mindset --Influenza.mindset = {"analytical", "pedantic", "cautious"} function Influenza:mindset() if not Influenza.mindset then Influenza.mindset = {} end send("mindset " .. Influenza.mindset[math.random(#Influenza.mindset)]) end --Attitude function Influenza:attitude() if not Influenza.attitude then Influenza.attitude = {} end Influenza.attitude = {"zealotry", "saintly", "lawyerly"} send("sm add attitude " .. Influenza.attitude[math.random(#Influenza.attitude)]) end --Imbue Esteem function Influenza:autoimbue() if not Influenza.imbue then Influenza.imbue = {} end if Influenza.imbue.settings == true and tonumber(gmcp.Char.Vitals.esteem) >= (Influenza.imbue.esteem * 1) and Influenza.imbue.settings2 == true then Influenza.imbue.settings2 = false send("imbue " ..Influenza.imbue.figurine.. " with " ..Influenza.imbue.esteem.. " esteem") end end --Beast Heal Ego --[[function Influenza:beastheal() if not Influenza.beastheal then Influenza.beastheal = {} end if not Influenza.beast.number then cecho("<red>You must first set which beast you wish to heal with! - INF BEAST <BEASTNUMBER>\n") end if Influenza.beast.setting == true and tonumber(gmcp.Char.Vitals) end end]] --Influenza Echos function Influenza:secho(self,args) cecho(string.format("%s", "\n<sienna>[<burlywood>Influenza<sienna>]<burlywood>: <SeaGreen>Info: <slate_grey>") .. args:title() .. "\n") end	--Saving settings function Influenza:saveConfig() table.save(getMudletHomeDir() .. "\\Influenza.lua", Influenza) cecho("\n\n <green>.oOo. <white>Influenza settings SAVED!!! <green> .oOo.\n") end --Influenza.saveconfig() --Loading settings from a saved table. If a saved table doesn't exist, it instead populates with default settings from Influenza:default() function Influenza:loadConfig() if Influenza.configged then return false end --if local file = getMudletHomeDir() .. "\\Influenza.lua" if not io.exists(file) then Influenza:default() cecho("\n <green>.oOo. <white>Influenza default settings LOADED!!! <green> .oOo.\n") end --if table.load(file, Influenza) cecho("\n <green>.oOo. <white>Influenza saved settings LOADED!!! <green> .oOo.\n") end --Influenza.loadConfig() --Setting Variables to their saved or default values if not Influenza.configged then Influenza:loadConfig() end --Search for Sound File in Profile Directory, if none is located, downloads it from the Influenza Dropbox. function Influenza:Sound() end
