                    <Script isActive="yes" isFolder="no">
                        <name>Influenza_Alpha</name>
                        <packageName></packageName>
                        <script>--Influenza System Settings

--Setting up namespace
if not Influenza then
	Influenza = {}
end

--Setting up default values
function Influenza:default()
	Influenza = {}
	Influenza.healing = {}
	--Influenza.healing.setting = false
	--Influenza.healing.sip = &quot;75&quot;
	--Influenza.healing.scroll = &quot;70&quot;
	--Influenza.healing.sparkle = &quot;65&quot;
	Influenza.target = {}
	Influenza.target.influence = &quot;None&quot;
	Influenza.target.debate = &quot;None&quot;
	Influenza.mindset = {&quot;analytical&quot;, &quot;pedantic&quot;, &quot;cautious&quot;}
	Influenza.debate = {&quot;pettifoggery&quot;, &quot;passion&quot;, &quot;pontification&quot;}
	Influenza.attitude = {&quot;zealotry&quot;, &quot;saintly&quot;, &quot;lawyerly&quot;} 
	Influenza.type = {}	
	Influenza.type.influence = &quot;none&quot;
	Influenza.influence = {
		charity = {&quot;begging&quot;, &quot;supplication&quot;, &quot;wheedling&quot;},
		empower = {&quot;compliments&quot;, &quot;admiration&quot;, &quot;praise&quot;},
		weaken = {&quot;teasing&quot;, &quot;mockery&quot;, &quot;derision&quot;},
		seduction = {&quot;flattery&quot;, &quot;charm&quot;, &quot;beguiling&quot;},
		paranoia = {&quot;rumors&quot;, &quot;distrust&quot;, &quot;conspiracies&quot;},
		amnesty = {&quot;amnesty&quot;, &quot;amnesty&quot;, &quot;amnesty&quot;},
		village = {
			Serenwilde = {&quot;lectures&quot;, &quot;recitation&quot;, &quot;oration&quot;},
			Celest = {&quot;proselytizing&quot;, &quot;preaching&quot;, &quot;evangelizing&quot;},
			Hallifax = {&quot;accordance&quot;, &quot;collaboration&quot;, &quot;assimilation&quot;},
			Gaudiguch = {&quot;revolution&quot;, &quot;liberty&quot;, &quot;freedon&quot;},
			Glomdoring = {&quot;manipulation&quot;, &quot;intrigue&quot;, &quot;trickery&quot;},
			Magnagora = {&quot;shock&quot;, &quot;awe&quot;, &quot;brainwash&quot;},
		}
	}
	Influenza.imbue = {}
	Influenza.imbue.settings = false
	Influenza.imbue.figurine = &quot;&quot;
	Influenza.imbue.esteem = &quot;&quot;
	Influenza.imbue.settings2 = false
	Influenza.echo = function (self, args) cecho(string.format(&quot;%s&quot;, &quot;\n&lt;sienna&gt;[&lt;burlywood&gt;Influenza&lt;sienna&gt;]&lt;burlywood&gt;: &lt;SeaGreen&gt;Info: &lt;slate_grey&gt;&quot;) .. args:title() .. &quot;\n&quot;) end
	--Beast Healing values
	Influenza.beast = {}
	Influenza.beast.number = &quot;nil&quot;
	Influenza.beast.setting = false
	Influenza.configged = true
	--Herb Stores
	Influenza.rift = {}
	Influenza.herbstore = {}
end

--Influenza Guts

--Influencing
function Influenza:influencer()
	if not Influenza.influence then
		Influenza.influence = {}
	end

	if not Influenza.type.influence then
		cecho(&quot;&lt;blue&gt;\nNo influence type selected&quot;)
	else
		if not lastinfluence then
				lastinfluence = 1
		elseif lastinfluence == 3 then
				lastinfluence = 1
		else
				lastinfluence = lastinfluence + 1
		end
		send(&quot;influence &quot; .. Influenza.target.influence .. &quot; with &quot; .. Influenza.influence[Influenza.type.influence][lastinfluence])
	end
end

--Debating
function Influenza:debate()
	if not Influenza.debate then
		Influenza.debate = {}
	end
	if not Influenza.target.debate then
		cecho(&quot;&lt;red&gt;\n\nNo Target Defined - DEBT &lt;PERSON&gt;!!&quot;)
	end
	send(&quot;debate &quot; ..Influenza.target.debate.. &quot; with &quot; .. Influenza.debate[math.random(#Influenza.debate)])
end

--Mindset
--Influenza.mindset = {&quot;analytical&quot;, &quot;pedantic&quot;, &quot;cautious&quot;}
function Influenza:mindset()
	if not Influenza.mindset then
		Influenza.mindset = {}
	end
	send(&quot;mindset &quot; .. Influenza.mindset[math.random(#Influenza.mindset)])
end

--Attitude
function Influenza:attitude()
	if not Influenza.attitude then
		Influenza.attitude = {}
	end
	Influenza.attitude = {&quot;zealotry&quot;, &quot;saintly&quot;, &quot;lawyerly&quot;}
	send(&quot;sm add attitude &quot; .. Influenza.attitude[math.random(#Influenza.attitude)])
end

--Imbue Esteem
function Influenza:autoimbue()
	if not Influenza.imbue then
		Influenza.imbue = {}
	end
	if Influenza.imbue.settings == true and tonumber(gmcp.Char.Vitals.esteem) &gt;= (Influenza.imbue.esteem * 1) and Influenza.imbue.settings2 == true then
		Influenza.imbue.settings2 = false
		send(&quot;imbue &quot; ..Influenza.imbue.figurine.. &quot; with &quot; ..Influenza.imbue.esteem.. &quot; esteem&quot;)
	end
end

--Beast Heal Ego
--[[function Influenza:beastheal()
	if not Influenza.beastheal then
		Influenza.beastheal = {}
	end
	if not Influenza.beast.number then
		cecho(&quot;&lt;red&gt;You must first set which beast you wish to heal with! - INF BEAST &lt;BEASTNUMBER&gt;\n&quot;)
	end
	if Influenza.beast.setting == true and tonumber(gmcp.Char.Vitals)
	end
end]]

--Influenza Echos
function Influenza:secho(self,args) 
cecho(string.format(&quot;%s&quot;, &quot;\n&lt;sienna&gt;[&lt;burlywood&gt;Influenza&lt;sienna&gt;]&lt;burlywood&gt;: &lt;SeaGreen&gt;Info: &lt;slate_grey&gt;&quot;) .. args:title() .. &quot;\n&quot;)
end	

--Saving settings
function Influenza:saveConfig()
   table.save(getMudletHomeDir() .. &quot;\\Influenza.lua&quot;, Influenza)
   cecho(&quot;\n\n          &lt;green&gt;.oOo.     &lt;white&gt;Influenza settings SAVED!!! &lt;green&gt;    .oOo.\n&quot;)
end --Influenza.saveconfig()


--Loading settings from a saved table.  If a saved table doesn't exist, it instead populates with default settings from Influenza:default()
function Influenza:loadConfig()
   if Influenza.configged then 
      return false 
   end --if

   local file = getMudletHomeDir() .. &quot;\\Influenza.lua&quot;
   if not io.exists(file) then 
      Influenza:default()
      cecho(&quot;\n          &lt;green&gt;.oOo.     &lt;white&gt;Influenza default settings LOADED!!! &lt;green&gt;    .oOo.\n&quot;)
   end --if

   table.load(file, Influenza)
   cecho(&quot;\n          &lt;green&gt;.oOo.     &lt;white&gt;Influenza saved settings LOADED!!! &lt;green&gt;    .oOo.\n&quot;)
end --Influenza.loadConfig()

--Setting Variables to their saved or default values
if not Influenza.configged then
   Influenza:loadConfig()
end

--Search for Sound File in Profile Directory, if none is located, downloads it from the Influenza Dropbox.
function Influenza:Sound()
end
	
