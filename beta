                    <script>function boxDisplay(msg, color)
	local leng = ((2*string.len(msg)) + 10)
	local mes = string.upper(msg)
	local col = &quot;&lt;red&gt;&quot;
	if color then
		col = (&quot;&lt;&quot;..color..&quot;&gt;&quot;)
	end

	cecho(col..&quot;\n &quot;)
	for i = 0,leng,1 do cecho(col..&quot;-&quot;) end
	cecho(col..&quot; \n|     &quot;..mes..&quot; &quot;..mes..&quot;     |&quot;)
	cecho(col..&quot;\n &quot;)
	for i = 0,leng,1 do cecho(col..&quot;-&quot;) end
	cecho(col..&quot; \n&quot;)
end

function deleteFull()
	deleteLine()
	tempLineTrigger(1,1,[[if isPrompt() then deleteLine() end]])
end</script>
