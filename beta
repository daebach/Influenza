i = {
echo	= function (self, args) cecho(string.format(&quot;%s&quot;, &quot;\n&lt;sienna&gt;[&lt;burlywood&gt;Influenza&lt;sienna&gt;]&lt;burlywood&gt;: &lt;SeaGreen&gt;Info: &lt;slate_grey&gt;&quot;) .. args:title() .. &quot;\n&quot;) end
